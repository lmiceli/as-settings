<application>
  <component name="DefaultFont">
    <option name="FONT_SIZE" value="18" />
    <option name="FONT_REGULAR_SUB_FAMILY" value="Regular" />
    <option name="FONT_BOLD_SUB_FAMILY" value="Bold" />
    <option name="USE_LIGATURES" value="true" />
  </component>
  <component name="EditorSettings">
    <option name="USE_SOFT_WRAPS" value="CONSOLE" />
    <option name="ARE_LINE_NUMBERS_SHOWN" value="false" />
    <option name="IS_INDENT_GUIDES_SHOWN" value="false" />
    <option name="IS_CAMEL_WORDS" value="true" />
    <option name="IS_WHEEL_FONTCHANGE_ENABLED" value="true" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\btodo\b.*" />
    <pattern pattern="\bfixme\b.*" />
    <pattern pattern="\bnocommit\b.*" />
    <pattern pattern="\bxxx\b.*" />
    <filter name="mine xxx">
      <pattern index="3" />
      <pattern index="2" />
    </filter>
  </component>
</application>